type Bank {
    id: ID
    name: String
}

input InputBank {
    name: String!
}

type QueryBank {
    getAllBank: [Bank]
    getBank(id: ID!): Bank
}

type MutationBank {
    createBank(input: InputBank!): Boolean
    updateBank(id: ID!, input: InputBank!): Boolean
    deleteBank(id: ID!): Boolean
}