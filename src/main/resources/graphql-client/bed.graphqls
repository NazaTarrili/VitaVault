type Bed {
    id: ID
    name: String
    bedStatus: BedStatus
    room: Room
}

input InputBed {
    name: String!
    bedStatus: ID!
    room: ID!
}

type QueryBed {
    getAllBed: [Bed]
    getBed(id: ID!): Bed
}

type MutationBed {
    createBed(input: InputBed!): Boolean
    updateBed(id: ID!, input: InputBed!): Boolean
    deleteBed(id: ID!): Boolean
}