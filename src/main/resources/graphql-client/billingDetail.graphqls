type BillingDetail {
    id: ID
    taxIdentifier: String
    email: String
    accountNumber: String
    reference: String
    iban: String
    phoneNumber: String
    bank: Bank
    person: Person
    address: Address
}

input InputBillingDetail {
    taxIdentifier: String!
    email: String!
    accountNumber: String!
    reference: String
    iban: String!
    phoneNumber: String!
    bank: ID!
    person: ID!
    address: ID!
}

type QueryBillingDetail {
    getAllBillingDetail: [BillingDetail]
    getBillingDetail(id: ID!): BillingDetail
}

type MutationBillingDetail {
    createBillingDetail(input: InputBillingDetail!): Boolean
    updateBillingDetail(id: ID!, input: InputBillingDetail!): Boolean
    deleteBillingDetail(id: ID!): Boolean
}