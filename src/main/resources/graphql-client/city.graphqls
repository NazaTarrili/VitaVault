type City {
    id: ID
    name: String
    state: State
    addresses: [Address]
}

input InputCity {
    name: String!
    state: ID!
}

type QueryCity {
    getAllCity: [City]
    getCity(id: ID!): City
}

type MutationCity {
    createCity(input: InputCity!): Boolean
    updateCity(id: ID!, input: InputCity!): Boolean
    deleteCity(id: ID!): Boolean
}