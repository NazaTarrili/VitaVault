type Country {
    id: ID
    name: String
    states: [State]
}

input InputCountry {
    name: String!
}

type QueryCountry {
    getAllCountry: [Country]
    getCountry(id: ID!): Country
}

type MutationCountry {
    createCountry(input: InputCountry!): Boolean
    updateCountry(id: ID!, input: InputCountry!): Boolean
    deleteCountry(id: ID!): Boolean
}