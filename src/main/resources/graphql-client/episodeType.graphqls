type EpisodeType {
    id: ID
    name: String
    episodes: [Episode]
}

input InputEpisodeType {
    name: String!
}

type QueryEpisodeType {
    getAll: [EpisodeType]
    getByID(id: ID!): EpisodeType
}

type MutationEpisodeType {
    create(input: InputEpisodeType!): Boolean
    update(id: ID!, input: InputEpisodeType!): Boolean
    delete(id: ID!): Boolean
}