type Plan {
    id: ID
    name: String
    socialWork: SocialWork
    patients: [Patient]
}

input InputPlan {
    name: String!
    socialWork: ID!
}

type QueryPlan {
    getAllPlan: [Plan]
    getPlan(id: ID!): Plan
}

type MutationPlan {
    createPlan(input: InputPlan!): Boolean
    updatePlan(id: ID!, input: InputPlan!): Boolean
    deletePlan(id: ID!): Boolean
}