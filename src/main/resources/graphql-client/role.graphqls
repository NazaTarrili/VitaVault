type Role {
    id: ID
    name: String
    users: [User]
}

input InputRole {
    name: String!
}

type QueryRole {
    getAllRole: [Role]
    getRole(id: ID!): Role
}

type MutationRole {
    createRole(input: InputRole!): Boolean
    updateRole(id: ID!, input: InputRole!): Boolean
    deleteRole(id: ID!): Boolean
}