type Room {
    id: ID
    name: String
    roomType: RoomType
    center: Center
    beds: [Bed]
}

input InputRoom {
    name: String!
    roomType: ID
    center: ID!
}

type QueryRoom {
    getAllRoom: [Room]
    getRoom(id: ID!): Room
}

type MutationRoom {
    createRoom(input: InputRoom!): Boolean
    updateRoom(id: ID!, input: InputRoom!): Boolean
    deleteRoom(id: ID!): Boolean
}