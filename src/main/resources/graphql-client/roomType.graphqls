type RoomType {
    id: ID
    name: String
    rooms: [RoomType]
}

input InputRoomType {
    name: String!
}

type QueryRoomType {
    getAllRoomType: [RoomType]
    getRoomType(id: ID!): RoomType
}

type MutationRoomType {
    createRoomType(input: InputRoomType!): Boolean
    updateRoomType(id: ID!, input: InputRoomType!): Boolean
    deleteRoomType(id: ID!): Boolean
}