type State {
    id: ID
    name: String
    country: Country
    cities: [City]
}

input InputState {
    name: String!
    country: ID!
}

type QueryState {
    getAll: [State]
    getByID(id: ID!): State
}

type MutationState {
    create(input: InputState!): Boolean
    update(id: ID!, input: InputState!): Boolean
    delete(id: ID!): Boolean
}