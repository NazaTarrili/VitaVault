type State {
    id: ID
    name: String
    country: Country
    cities: [City]
}

input InputState {
    name: String!
    country: ID!
}

type QueryState {
    getAllState: [State]
    getState(id: ID!): State
}

type MutationState {
    createState(input: InputState!): Boolean
    updateState(id: ID!, input: InputState!): Boolean
    deleteState(id: ID!): Boolean
}