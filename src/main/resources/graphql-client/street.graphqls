type Street {
    id: ID
    name:String
    addresses: [Address]
}

input InputStreet {
    name:String!
}

type QueryStreet {
    getAllStreet: [Street]
    getStreet(id: ID!): Street
}

type MutationStreet {
    createStreet(input: InputStreet!): Boolean
    updateStreet(id: ID!, input: InputStreet!): Boolean
    deleteStreet(id: ID!): Boolean
}