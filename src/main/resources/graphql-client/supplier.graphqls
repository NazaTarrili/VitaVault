type Supplier {
    id: ID
    name: String
    address: Address
    supplies: [Supply]
}

input InputSupplier {
    name: String!
    address: ID!
}

type QuerySupplier {
    getAllSupplier: [Supplier]
    getSupplier(id: ID!): Supplier
}

type MutationSupplier {
    createSupplier(input: InputSupplier!): Boolean
    updateSupplier(id: ID!, input: InputSupplier!): Boolean
    deleteSupplier(id: ID!): Boolean
}