type City {
    id: ID
    name: String
    state: State
    addresses: [Address]
}

input InputCity {
    name: String!
    state: ID!
}

type QueryCity {
    getAll: [City]
    getByID(id: ID!): City
}

type MutationCity {
    create(input: InputCity!): Boolean
    update(id: ID!, input: InputCity!): Boolean
    delete(id: ID!): Boolean
}