type Patient {
    id: ID
    appointments: [Appointment]
    episodes: [Episode]
    person: Person
    plan: Plan
}

input InputPatient {
    person: ID!
    plan: ID!
}

type QueryPatient {
    getAll: [Patient]
    getByID(id: ID!): Patient
}

type MutationPatient {
    create(input: InputPatient!): Boolean
    update(id: ID!, input: InputPatient!): Boolean
    delete(id: ID!): Boolean
}