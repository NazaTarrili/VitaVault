type RoomType {
    id: ID
    name: String
    rooms: [RoomType]
}

input InputRoomType {
    name: String!
}

type QueryRoomType {
    getAll: [RoomType]
    getByID(id: ID!): RoomType
}

type MutationRoomType {
    create(input: InputRoomType!): Boolean
    update(id: ID!, input: InputRoomType!): Boolean
    delete(id: ID!): Boolean
}