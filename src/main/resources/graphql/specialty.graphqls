type Specialty {
    id: ID
    name: String
    schedules: [Schedule]
}

input InputSpecialty {
    name: String!
}

type QuerySpecialty {
    getAll: [Specialty]
    getByID(id: ID!): Specialty
}

type MutationSpecialty {
    create(input: InputSpecialty!): Boolean
    update(id: ID!, input: InputSpecialty!): Boolean
    delete(id: ID!): Boolean
}